{
  "name": "stream-manager",
  "version": "1.0.0",
  "type": "module",
  "description": "Stream Processing Service for NFT Auction System",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "dev:debug": "DEBUG=stream:* NODE_OPTIONS='--inspect' tsx watch src/index.ts",
    "dev:profile": "NODE_OPTIONS='--prof' tsx src/index.ts",
    "dev:memory": "NODE_OPTIONS='--inspect --heap-prof' tsx src/index.ts",
    "dev:network": "DEBUG=stream:network,stream:rtmp,stream:ws tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "del-cli dist",
    "prebuild": "npm run clean",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:e2e": "jest --selectProjects e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:perf": "tsx tools/perf/run-all.ts",
    "test:perf:stream": "tsx src/tools/perf/stream-performance.ts",
    "test:perf:workers": "tsx tools/perf/worker-performance.ts",
    "test:perf:memory": "tsx tools/perf/memory-analysis.ts",
    "test:perf:network": "tsx tools/perf/network-performance.ts",
    "test:load": "tsx tools/perf/load-test.ts",
    "test:stress": "tsx tools/perf/stress-test.ts",
    "test:memory:leaks": "NODE_OPTIONS='--inspect --heap-prof' tsx tools/memory/leak-detection.ts",
    "test:memory:heap": "tsx tools/memory/heap-snapshot.ts",
    "test:memory:gc": "NODE_OPTIONS='--trace-gc' tsx tools/memory/gc-analysis.ts",
    "test:memory:timeline": "tsx tools/memory/memory-timeline.ts",
    "test:frames:timing": "tsx tools/frames/timing-analysis.ts",
    "test:frames:drops": "tsx tools/frames/drop-analysis.ts",
    "test:frames:buffer": "tsx tools/frames/buffer-analysis.ts",
    "test:frames:quality": "tsx tools/frames/quality-metrics.ts",
    "debug:stream": "tsx src/tools/debug/generate-test-stream.ts",
    "debug:workers": "DEBUG=stream:worker:* tsx tools/debug/worker-debug.ts",
    "debug:frames": "DEBUG=stream:frames:* tsx tools/debug/frame-debug.ts",
    "debug:heap": "node --inspect --heap-prof tools/debug/generate-heap-snapshot.js",
    "debug:leaks": "tsx tools/debug/memory-leak-detector.ts",
    "debug:timing": "tsx tools/debug/frame-timing-analyzer.ts",
    "debug:tasks": "tsx tools/debug/worker-task-analyzer.ts",
    "debug:rtmp": "DEBUG=stream:rtmp:* tsx tools/debug/rtmp-analyzer.ts",
    "debug:ws": "DEBUG=stream:ws:* tsx tools/debug/websocket-analyzer.ts"
  },
  "dependencies": {
    "@types/ioredis": "^4.28.10",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "ioredis": "^5.5.0",
    "node-media-server": "^2.6.2",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.3",
    "prom-client": "^15.1.3",
    "redis": "^4.6.12",
    "sharp": "^0.33.5",
    "twitter-api-v2": "^1.15.2",
    "uuid": "^11.0.5",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.8",
    "@babel/preset-typescript": "^7.26.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.16",
    "@types/node-media-server": "^2.3.5",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.56.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
