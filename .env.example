# General
NODE_ENV=development
LOG_LEVEL=debug

# Redis
REDIS_PASSWORD=change_me_in_production
REDIS_HOST=redis
REDIS_PORT=6379

# Twitter API
TWITTER_USERNAME=your_username
TWITTER_PASSWORD=your_password
TWITTER_EMAIL=your_email
TWITTER_API_KEY=your_api_key
TWITTER_API_SECRET=your_api_secret

# Shape L2
WALLET_SECRET_SALT=change_me_in_production
SOL_RPC_URL=https://api.mainnet-beta.solana.com
SHAPE_L2_NETWORK=testnet
SHAPE_L2_CONTRACT=your_contract_address

# ElizaOS
DISCORD_APPLICATION_ID=your_app_id
DISCORD_API_TOKEN=your_token
ELEVENLABS_XI_API_KEY=your_key
TEE_MODE=development

# Monitoring
GRAFANA_PASSWORD=admin_in_dev_change_in_prod
PROMETHEUS_RETENTION_DAYS=15

# SSL/TLS (Production Only)
SSL_CERT_PATH=/etc/nginx/ssl/cert.pem
SSL_KEY_PATH=/etc/nginx/ssl/key.pem

# Backup (Production Only)
BACKUP_S3_BUCKET=your-backup-bucket
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
BACKUP_RETENTION_DAYS=30

# Resource Limits
MAX_MEMORY_LIMIT=2G
MAX_CPU_LIMIT=1.0

# Security
ALLOWED_IPS=127.0.0.1,your.ip.address.here
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000

# Feature Flags
USE_MOCK_APIS=true
USE_MOCK_BLOCKCHAIN=true
ENABLE_DEBUG_ENDPOINTS=true

# Monitoring Configuration
MONITORING_DOMAIN=monitoring.yourdomain.com
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=your_secure_password
ACME_EMAIL=your@email.com 